function mod(x, y) local q = x / y return x - q * y end

function fill(n) for i = 0, n - 1 do set(i, mod(54 + i * i * i, 42)) end end

function swap(i, j) local aux = get(i) set(i, get(j)) set(j, aux) end

function sort(l, r) if l >= r then return end local pivot = get(r) local i, j, k = l, l, r - 1 while j <= k do if get(j) < pivot then swap(j, i) i = i + 1 j = j + 1 elseif get(j) == pivot then j = j + 1 else swap(j, k) k = k - 1 end end swap(r, j) sort(l, i - 1) sort(j + 1, r) end

function show(n) for i = 0, n - 1 do print(get(i)) end end

fill(100) sort(0, 99) show(100)
